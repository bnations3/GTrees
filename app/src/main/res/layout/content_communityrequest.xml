<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".communityrequest"
    tools:showIn="@layout/activity_communityrequest">

    <EditText
        android:id="@+id/title4"
        style="@android:style/TextAppearance.DeviceDefault.WindowTitle"
        android:layout_width="306dp"
        android:layout_height="55dp"
        android:autoText="false"
        android:background="@android:color/transparent"
        android:ems="10"
        android:freezesText="false"
        android:inputType="none|textPersonName"
        android:selectAllOnFocus="false"
        android:text="New Community Request"
        android:textAppearance="@style/TextAppearance.AppCompat.Title"
        android:textColor="@android:color/holo_green_dark"
        android:textColorHint="@android:color/holo_green_light"
        android:textStyle="normal|bold"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:fontFamily="casual"
        tools:layout_editor_absoluteY="30dp"
        tools:textSize="24sp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="wrap_content"
        android:layout_height="51dp"
        android:ems="10"
        android:inputType="textUri|textPersonName"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="125dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Project Name"
        android:textColor="@android:color/holo_green_dark"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="100dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Project Name"
        android:textColor="@android:color/holo_green_dark"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="100dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="163dp"
        android:layout_height="28dp"
        android:text="Description of Event"
        android:textColor="@android:color/holo_green_dark"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="183dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="31dp"
        android:text="Location"
        android:textColor="@android:color/holo_green_dark"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="268dp" />

    <EditText
        android:id="@+id/editText7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPostalAddress"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="306dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Date"
        android:textColor="@android:color/holo_green_dark"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="353dp" />

    <EditText
        android:id="@+id/editText8"
        android:layout_width="150dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="date"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="395dp" />

    <EditText
        android:id="@+id/editText9"
        android:layout_width="150dp"
        android:layout_height="42dp"
        android:ems="10"
        android:inputType="time"
        tools:layout_editor_absoluteX="195dp"
        tools:layout_editor_absoluteY="396dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Time"
        android:textColor="@android:color/holo_green_dark"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="195dp"
        tools:layout_editor_absoluteY="353dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit Request"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="453dp" />

    <EditText
        android:id="@+id/editText10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textMultiLine"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="211dp" />
</android.support.constraint.ConstraintLayout>